{
    "presets": [
        "module:metro-react-native-babel-preset"
    ],
    "plugins": [
        "@babel/transform-react-jsx-source",
        [
            "module-resolver",
            {
                "extensions": [
                    ".js",
                    ".ios.js",
                    ".android.js",
                    ".tsx",
                    ".ts"
                ],
                "alias": {
                    "@ui": "./src/ui",
                    "@assets": "./src/assets",
                    "@api": "./src/api",
                    "@service": "./src/services",
                    "@img": "./src/img"
                }
            }
        ]
    ]
}